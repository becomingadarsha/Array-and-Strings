#include<stdio.h>
int main()
 {
   char str[100], rev,count=0;
   int i=0,c,j;
   printf("enter the string :\n");
   gets(str);
   for(i=0;str[i]!='\0';i++)
	{
			count++;
	}

   j = count-1;
    for(i=0;i<j;i++,j--)
    {
	
	  rev = str[i];
      str[i] = str[j];
      str[j] = rev;
   }
 
   printf("Reverse string is\n%s",str);
   return 0;
}
