//alphabetical order of names given aman,akesh,anish,anam,anil,anju
#include<stdio.h>
#include<string.h>
int main()
{
	int i,j,n;
	char str[100][100],temp[100];
	printf("Enter name one by one:");
	for(i=0;i<6;i++)
	{
		gets(str[i]);
	}
	for(i=0;i<6;i++)
	for(j=i+1;j<6;j++)
	{
         if(strcmp(str[i],str[j])>0)
		 {
            strcpy(temp,str[i]);
            strcpy(str[i],str[j]);
            strcpy(str[j],temp);
         }
      }
      printf("\nIn alphabetical order:\n");
      for(i=0;i<6;i++)
      {
      	puts(str[i]);
	  }
	  return 0;
}
